import sqlite3

conexion = sqlite3.connect('base de datos/MenuesBBDD.db')

cursor = conexion.cursor()

sentenciaSQL = "INSERT INTO menues "
sentenciaSQL = sentenciaSQL + "VALUES ('Pizza de Muzzarella', 3200, salsa de tomate y queso muzzarella)"


cursor.execute(sentenciaSQL)

conexion.commit()
conexion.close()
